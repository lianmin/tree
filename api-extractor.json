{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/es2017/index.d.ts",
  "bundledPackages": [],
  "apiReport": {
    "enabled": true,
    "reportFileName": "API.md",
    "reportFolder": "./",
    "reportTempFolder": "node_modules"
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": false,
    "omitTrimmingComments": true
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    }
  }
}
