{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./es2017/index.d.ts",

  "bundledPackages": [],

  "apiReport": {
    "enabled": true,
    "reportFileName": "api.md",
    "reportFolder": "./docs",
    "reportTempFolder": "./.docusaurus"
  },

  "docModel": {
    "enabled": false
  },

  "dtsRollup": {
    "enabled": false,
    "omitTrimmingComments": true
  },

  "tsdocMetadata": {
    "enabled": false
  },

  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    }
  }
}
